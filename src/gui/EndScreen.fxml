<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="553.0" prefWidth="759.0" stylesheets="@resources/main.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.EndController">
    <children>
        <ImageView fitHeight="90.0" fitWidth="102.0" layoutX="47.0" layoutY="16.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@resources/logo.png" />
            </image>
        </ImageView>
        <Label alignment="CENTER" layoutX="148.0" layoutY="27.0" prefHeight="67.0" prefWidth="484.0" text="Thank you!" textAlignment="JUSTIFY" textFill="WHITE">
            <font>
                <Font name="Lato Light" size="53.0" />
            </font>
        </Label>
        <Label alignment="CENTER" layoutX="81.0" layoutY="119.0" prefHeight="67.0" prefWidth="598.0" text="All done!" textAlignment="JUSTIFY" textFill="WHITE">
            <font>
                <Font name="Lato Light" size="22.0" />
            </font>
        </Label>
        <Button fx:id="exitBtn" layoutX="663.0" layoutY="-1.0" mnemonicParsing="false" onAction="#exit" prefHeight="56.0" prefWidth="95.0" styleClass="exitBtn" text="X" textAlignment="CENTER" textFill="#ffffff78">
            <font>
                <Font name="Lato Light" size="28.0" />
            </font>
        </Button>
        <Label alignment="CENTER" layoutX="91.0" layoutY="164.0" prefHeight="67.0" prefWidth="598.0" text="Listen to your censored song below:" textAlignment="JUSTIFY" textFill="WHITE">
            <font>
                <Font name="Lato Light" size="22.0" />
            </font>
        </Label>
        <Label fx:id="warningLabel" alignment="CENTER" layoutX="217.0" layoutY="502.0" prefHeight="42.0" prefWidth="346.0" text="Please select a censoring option!" textAlignment="JUSTIFY" textFill="WHITE" visible="false">
            <font>
                <Font name="Lato Light" size="15.0" />
            </font>
        </Label>
      <Button fx:id="playBtn" layoutX="236.0" layoutY="240.0" mnemonicParsing="false" onAction="#play" prefHeight="67.0" prefWidth="125.0" styleClass="uploadBtn" text="Play" textFill="#ffffff78">
         <font>
            <Font name="Lato Light" size="22.0" />
         </font>
      </Button>
      <Label fx:id="audioFileName" alignment="CENTER" layoutX="94.0" layoutY="301.0" prefHeight="67.0" prefWidth="598.0" textAlignment="JUSTIFY" textFill="WHITE">
         <font>
            <Font name="Lato Light" size="22.0" />
         </font>
      </Label>
      <Button fx:id="stopBtn" disable="true" layoutX="400.0" layoutY="241.0" mnemonicParsing="false" onAction="#stop" prefHeight="67.0" prefWidth="125.0" styleClass="uploadBtn" text="Stop" textFill="#ffffff78">
         <font>
            <Font name="Lato Light" size="22.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
